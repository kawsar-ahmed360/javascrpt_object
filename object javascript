var obj ={
    x:10,
    y:20,
    z:30
}

var obj1 ={
    x:10,
    y:20,
    z:30
}

//----------Loop Object-------

for(var i in obj){
    console.log(i+obj[i]);
}

console.log(Object.keys(obj)); // return object key
console.log(Object.values(obj)); // return object value
console.log(Object.entries(obj)); // array object return

var obj3 = Object.assign(obj1);
obj3.x=1400;

console.log(obj3);
